    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-refresh": "^0.11.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.18.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.3.0",
